<!-- Vidoegular Videos-->
<div class="col-partial2">
    <h2 id ="viewTwoMessage">{{viewTwoMessage}}</h2>
    <div>

        <ng-youtube width=400 fullscreenctrl="false" backlight="false" volumectrl="false" skin="mejs-wmp" myid="player1" src="https://www.youtube.com/watch?v=UlvCbnKAH3g"></ng-youtube>
        <div>Helo from two </div>
        <ng-youtube backlight="false" poster="https://dl.dropboxusercontent.com/s/ny50ry0yynncrwh/oceans-clip.png?dl=0g" width=350 myid="player2" src="http://video-js.zencoder.com/oceans-clip.mp4"></ng-youtube> 


        <ng-youtube backlight="false" width=350 myid="player4" src="/assets/moviem.mp4" type="video/mp4" ></ng-youtube




        <div>Helo from three </div>
        <ng-youtube backlight="false" poster="https://dl.dropboxusercontent.com/s/q97xk0pxart3629/earth.png?dl=0" width=350 myid="player3" src="http://www.videogular.com/assets/videos/videogular.mp4"></ng-youtube>
    </div>
    <ipsum></ipsum>
    <h3 id ="videoOneTitle">Ocean View</h3>
    <!--Ocean View Video-->
    <div class =vgOuterContainer ng-mouseleave="vgMouseLeave()">
        <!--videogular video panel Ocean View-->
        <div class="vg-container" ng-style="style(1)" id ="videoOne">
            <videogular
            ng-class ="setVgClass ? 'vg-video-one ':'{{vgFalse}} '"
            vg-width="config.width"
            vg-height="config.height"
            vg-theme="config.theme"
            ng-mouseover="vgMouseOver()"
            id ="videoregularOne">
            <video class='videoPlayer'
                vg-src="config.sources"
                preload='none'>
            </video>
            <vg-controls  class = vgControls
                vg-autohide="config.autoHide"
                vg-autohide-time="config.autoHideTime"
                style="height: 50px;">
                <!-- First element from the left -->
                <vg-play-pause-button id =playPause_ov></vg-play-pause-button>
                <vg-timeDisplay>{{ currentTime}}</vg-timeDisplay>
                <vg-scrubBar>
                    <vg-scrubbarcurrenttime></vg-scrubbarcurrenttime>
                </vg-scrubBar>
                <vg-timeDisplay>{{ totalTime }}</vg-timeDisplay>
                <vg-volume>
                    <vg-mutebutton></vg-mutebutton>
                    <vg-volumebar></vg-volumebar>
                </vg-volume>
                <vg-fullscreenButton ng-click="onFullScreen(1)"></vg-fullscreenButton>
            </vg-controls>
            <vg-buffering></vg-buffering>
            <vg-overlay-play></vg-overlay-play>
            <vg-poster-image
                vg-url='config.plugins.poster.url'
                vg-stretch="config.stretch.value">
            </vg-poster-image>
            </videogular>
        </div><!--end videogular video panel Ocean View-->
        <!--Videogular Bindings Control Panel Ocean View-->
        <div  ng-class ="setVgControlsTwoClass ? 'vgControlsTwo  vgControlsHidden':'vgControlsTwo '" id="videoOneControls">
            <div class="row" >
                <div class="small-12 columns">
                    <dl class="sub-nav">
                        <dd>&nbsp;</dd>
                        <dd>
                            <button class ="button tiny" ng-click ="showLargeSmall(1)"
                                id ="lgeSmall_oceanView">
                                <span ng-show="makeVideoSmall">Smaller View</span>
                                <span ng-show = "makeVideoLarge">Larger View</span>
                            </button>
                        </dd>
                        <dd>
                            <div>
                                <input id="widthInput"
                                class="form-control ng-pristine ng-valid"
                                type="text"
                                ng-model="config.width">
                                <label for="widthInput"  id ="width_label_ocean_view">Width</label>
                            </div>
                        </dd>
                        <dd>
                            <div>
                                <input id="heightInput"
                                class="form-control ng-pristine ng-valid"
                                type="text"
                                ng-model="config.height">
                                <label for="heightInput" id ="height_label_ocean_view">Height</label>
                            </div>
                        </dd>
                        <dd>
                            <div>
                                <label class="checkbox">
                                    <input id="autoHideInput"
                                    class="ng-valid ng-dirty"
                                    type="checkbox"
                                    ng-model="config.autoHide">
                                    Autohide
                                </label>
                            </div>
                        </dd>
                        <dd></dd>
                        <dd></dd>
                        <dd></dd>
                    </dl>
                </div>
            </div>
        </div>
    </div><!--end Videogular Outer Container Ocean View-->
    <ipsum></ipsum>
</div>
